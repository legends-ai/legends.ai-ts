    "koa-router": "^7.2.1",
    "koa-static": "^4.0.1",
    "maxmind": "^2.2.0",
    "normalize.css": "^7.0.0",
    "query-string": "^5.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-relay": "^1.1.0",
    "relay-runtime": "^1.1.0",
    "universal-router": "^3.2.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.0",
    "@types/history": "^4.6.0",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/koa": "^2.0.39",
    "@types/koa-bodyparser": "^3.0.23",
    "@types/koa-router": "^7.0.22",
    "@types/koa-static": "^3.0.0",
    "@types/maxmind": "^2.0.2",
    "@types/prop-types": "^15.5.1",
    "@types/query-string": "^5.0.0",
    "@types/react": "^15.6.0",
    "@types/react-dom": "^15.5.1",
    "@types/react-relay": "^0.9.13",
    "@types/webpack": "^3.0.5",
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "^3.2.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-relay": "^1.1.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
