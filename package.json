{
  "name": "legends.ai",
  "private": true,
  "scripts": {
    "lint": "eslint --ext ts,tsx src/ components/",
    "clean": "rm -rf build && rm -rf types/components",
    "pull-geodata": "scripts/update_geodata.sh",
    "pull-schema": "scripts/update_graphql.sh",
    "tcm": "tcm src -c -o types",
    "relay-compiler": "relay-compiler --src ./src --schema schema.graphql --extensions ts tsx",
    "devbuild": "npm run clean && npm run tcm && webpack --env.DEBUG",
    "start": "npm run devbuild && node build/server.js"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "history": "^4.6.3",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.3.0",
