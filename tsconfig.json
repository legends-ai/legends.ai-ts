{
  "compilerOptions": {
    "outDir": "build",
    "sourceMap": true,
    "strict": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "src": ["src"],
      "src/*": ["src/*"]
    }
  },
  "include": [
    "./src/**/*"
  ]
}
